<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.atto.developers.atto.fragment.MypageFragment">


    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        layout="@layout/view_mypage_headerview"
        android:layout_gravity="left|top"
        android:id="@+id/mypage_header" />
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/rv_list"
        android:layout_above="@+id/include2"
        android:layout_below="@+id/mypage_header" />

    <include
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        layout="@layout/view_mypage_maker_footerview"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/view_mypage_maker_footerview" />
</RelativeLayout>
