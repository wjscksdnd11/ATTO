<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/image_mypage_profile"
        android:background="@drawable/image_shape"
        android:layout_margin="@dimen/activity_horizontal_margin"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>

    <TextView

        android:singleLine="true"
        android:ellipsize="end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/text_mypage_nickname"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/image_mypage_profile"
        android:layout_toEndOf="@+id/image_mypage_profile"
        android:layout_marginLeft="33dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="33dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/text_mypage_phonenumber"
        android:layout_below="@+id/text_mypage_nickname"
        android:layout_alignLeft="@+id/text_mypage_nickname"
        android:layout_alignStart="@+id/text_mypage_nickname" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/text_mypage_address"
        android:layout_below="@+id/text_mypage_phonenumber"
        android:layout_alignLeft="@+id/text_mypage_phonenumber"
        android:layout_alignStart="@+id/text_mypage_phonenumber" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_mypage_setting_myprofile"
        android:id="@+id/btn_mypage_setting_myprofile"
        android:layout_below="@+id/image_mypage_profile"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MY거래"
        android:id="@+id/textView8"
        android:layout_below="@+id/btn_mypage_setting_myprofile"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="거래더보기>"
        android:id="@+id/text_my_trade"
        android:layout_alignTop="@+id/textView8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>